    "compare-versions": "^4.1.3",
    "csv-stringify": "2.0.1",
    "electron-is-dev": "2.0.0",
    "electron-log": "4.3.0",
    "fs-extra": "5.0.0",
    "immup": "3.0.0",
    "js-yaml": "3.10.0",
