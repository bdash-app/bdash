{
  "private": true,
  "name": "Bdash",
  "version": "1.4.0",
  "productName": "Bdash",
  "description": "A simple business intelligence application.",
  "homepage": "https://github.com/bdash-app/bdash",
  "author": {
    "name": "Kazuhito Hokamura",
    "email": "k.hokamura@gmail.com"
  },
  "scripts": {
    "start": "electron app/development",
    "watch": "webpack --watch",
    "test": "yarn run test:unit && yarn run test:integration",
    "test:unit": "electron-mocha --grep @remote --invert \"./test/unit/**/*.test.{ts,tsx}\"",
    "test:build": "webpack --env.BUILD_ENV=test && electron-builder build --config electron-builder.test.yml --publish never --dir",
    "test:integration": "yarn run test:build && mocha --exit --grep @remote --invert test/integration/test.ts",
    "lint": "yarn run lint:eslint && yarn run lint:tsc && yarn run lint:prettier",
    "lint:eslint": "eslint --ext '.js,.jsx,.ts,.tsx' .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --list-different ./**/*.{js,jsx,ts,tsx,css,json}",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,json}'",
    "publish": "webpack --env.BUILD_ENV=production && electron-builder build --publish always",
    "ci": "yarn run lint && yarn run test"
  },
  "dependencies": {
    "@google-cloud/bigquery": "0.7.0",
    "aws-sdk": "2.259.1",
    "classnames": "2.2.5",
    "codemirror": "5.33.0",
    "csv-stringify": "2.0.1",
    "electron-is-dev": "0.3.0",
    "electron-log": "2.2.14",
    "electron-updater": "2.21.10",
    "font-awesome": "4.7.0",
    "fs-extra": "5.0.0",
    "immup": "3.0.0",
    "js-yaml": "3.10.0",
    "lodash": "4.17.10",
    "markdown-table": "1.1.2",
    "moment": "2.20.1",
    "mysql2": "1.5.3",
    "pg": "7.4.3",
    "plotly.js": "1.32.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-micro-flyout": "1.0.1",
    "react-modal": "3.1.10",
    "react-select": "1.2.1",
    "sqlite3": "4.0.0",
    "td": "0.3.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.3",
    "@types/codemirror": "0.0.55",
    "@types/csv-stringify": "1.4.1",
    "@types/mocha": "2.2.46",
    "@types/node": "9.3.0",
    "@types/plotly.js": "1.28.10",
    "@types/react": "16.0.34",
    "@types/react-dom": "16.0.3",
    "@types/react-select": "1.1.0",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.8",
    "electron": "2.0.2",
    "electron-builder": "20.8.1",
    "electron-mocha": "6.0.1",
    "electron-publisher-s3": "20.14.4",
    "eslint": "4.16.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-react": "7.5.1",
    "execa": "0.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "prettier": "1.10.2",
    "spectron": "3.7.2",
    "ts-loader": "3.2.0",
    "ts-node": "5.0.1",
    "typescript": "2.9.1",
    "typescript-eslint-parser": "15.0.0",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-node-externals": "1.6.0"
  },
  "license": "MIT"
}
